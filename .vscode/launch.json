{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Angular",
      "type": "chrome",
      "request": "launch",
      "preLaunchTask": "Serve Frontend", // task of task.json
      "url": "http://localhost:4200/#",
      "sourceMaps": true,
      "webRoot": "${workspaceFolder}",
      "resolveSourceMapLocations": [
        "${workspaceFolder}/**",
        "!**/node_modules/**"
      ]
    }
  ]
}
